{
    "openapi": "3.0.0",
    "info": {
        "title": "Frappe API",
        "version": "1.0.0"
    },
    "paths": {
        "/api/method/jsk.api.auth.register": {
            "post": {
                "summary": "Register",
                "tags": [
                    "auth"
                ],
                "parameters": [],
                "requestBody": {
                    "description": "Request body",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "first_name": {
                                        "description": "First name is required and should be alphabetic.",
                                        "maxLength": 25,
                                        "pattern": "^[a-zA-Z]+$",
                                        "title": "First Name",
                                        "type": "string"
                                    },
                                    "last_name": {
                                        "anyOf": [
                                            {
                                                "maxLength": 25,
                                                "type": "string"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "default": null,
                                        "description": "Last name should be a maximum of 25 characters.",
                                        "title": "Last Name"
                                    },
                                    "mobile": {
                                        "description": "Mobile number is required and should be exactly 10 digits.",
                                        "maxLength": 10,
                                        "minLength": 10,
                                        "pattern": "^\\d+$",
                                        "title": "Mobile",
                                        "type": "string"
                                    },
                                    "email": {
                                        "description": "Email is required and should be a valid email address.",
                                        "title": "Email",
                                        "type": "string"
                                    },
                                    "firebase_token": {
                                        "description": "Firebase token is required.",
                                        "title": "Firebase Token",
                                        "type": "string"
                                    },
                                    "platform": {
                                        "anyOf": [
                                            {
                                                "type": "string"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "default": null,
                                        "description": "Platform is optional.",
                                        "title": "Platform"
                                    },
                                    "app_version": {
                                        "anyOf": [
                                            {
                                                "type": "string"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "default": null,
                                        "description": "App version is optional.",
                                        "title": "App Version"
                                    }
                                },
                                "required": [
                                    "first_name",
                                    "mobile",
                                    "email",
                                    "firebase_token"
                                ],
                                "title": "RegisterUser",
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "basicAuth": []
                    }
                ]
            }
        },
        "/api/method/jsk.api.auth.validate_request": {
            "post": {
                "summary": "Validate Request",
                "tags": [
                    "auth"
                ],
                "parameters": [],
                "requestBody": null,
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "basicAuth": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "basicAuth": {
                "type": "http",
                "scheme": "basic"
            }
        }
    },
    "security": [
        {
            "basicAuth": []
        }
    ]
}